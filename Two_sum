class Solution:
    
  def twoSum(self, nums: List[int], target: int) -> List[int]:
  #def twoSum(self, nums, target):
      nums_len = len(nums)

      # generate dict
      dict = {}

      for i in range(len(nums)):
          value = target - nums[i]
          if(dict.get('%d' %value)!=None and dict.get('%d' %value) < i):
              break
          else:
              dict['%d' %nums[i]] = i

      return i, dict.get('%d' %value)
        
